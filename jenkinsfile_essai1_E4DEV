pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'construction du pipeline'
            }
        }
        stage('Test') {
            steps {
                echo 'phase test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploiment'
            }
        }
    }
}
